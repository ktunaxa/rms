<div>
	<h1>Create referral</h1>
	<table>
		<tr>
			<td>
				<label>
					Referral id:<br/>
					<input type="text" name="referralId" value=""/>
					<input type="hidden" name="referralId_required" value="true"/>
					<input type="hidden" name="referralId_type" value="String"/>
				</label><br/>
			</td>
		</tr>
		<tr>
			<td>
				<label>
					Description:<br/>
					<textarea name="description" value=""></textarea>
					<input type="hidden" name="description_required" value="false"/>
					<input type="hidden" name="description_type" value="String"/>
				</label><br/>
			</td>
		</tr>
		<tr>
			<td>
				<label>
					Referral e-mail:<br/>
					<input type="text" name="requesterEmail" value=""/>
					<input type="hidden" name="requesterEmail_required" value="true"/>
					<input type="hidden" name="requesterEmail_type" value="String"/>
				</label><br/>
			</td>
		</tr>
		<tr>
			<td>
				<label>
					Engagement level:<br/>
					<input type="number" name="engagementLevel" value="0" min="3"/>
					<input type="hidden" name="engagementLevel_type" value="Integer"/>
				</label>
			</td>
		</tr>
	</table>
</div>