<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<bean id="googleSatLayer" class="org.geomajas.configuration.client.ClientRasterLayerInfo">
		<property name="serverLayerId" value="googleSat" />
		<property name="label" value="Google satellite layer" />
		<property name="visible" value="true" />
		<property name="style" value="1" />
	</bean>

	<bean name="googleSat" class="org.geomajas.layer.google.GoogleLayer" >
		<property name="satellite" value="true" />
		<property name="maxZoomLevel" value="19" />
	</bean>

</beans>